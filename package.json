{
  "name": "rsc-demo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">= 20.12.2"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.config.js",
    "start": "NODE_ENV=production node --conditions=react-server --enable-source-maps ./dist/server.js",
    "watch": "rimraf dist && webpack --config ./webpack.config.js --watch",
    "check": "biome check --write",
    "format": "biome format --write"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@fastify/compress": "^7.0.3",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "close-with-grace": "^1.3.0",
    "fastify": "^4.28.0",
    "pino-pretty": "^11.2.1",
    "react": "rc",
    "react-dom": "rc",
    "react-error-boundary": "^4.0.13",
    "react-server-dom-webpack": "rc"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@swc/core": "1.6.3",
    "@types/node": "^20.14.5",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "browserslist": "^4.23.1",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.37.1",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon-webpack-plugin": "^4.8.2",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "swc-loader": "^0.2.6",
    "typescript": "^5.4.2",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@rc",
      "@types/react-dom": "npm:types-react-dom@rc"
    }
  },
  "packageManager": "pnpm@9.4.0"
}
